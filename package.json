  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltennstedt/finnjs.git"
  },
  "keywords": [
    "math",
    "mathematics",
    "vector",
    "matrix",
    "vectors",
    "matrices",
    "complex",
    "number",
    "numbers",
    "fraction",
    "fractions",
    "circle",
    "circles",
    "rectangle",
    "rectangles"
  ],
  "author": "Lars Tennstedt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ltennstedt/finnjs/issues"
  },
  "homepage": "https://github.com/ltennstedt/finnjs#readme",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "extensions": [
      "js"
    ],
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "typescript": "4.5.5",
    "eslint": "8.7.0",
    "jshint": "2.13.4",
    "ava": "4.0.1",
    "nyc": "15.1.0",
    "jsdoc": "3.6.10"
  }
}
